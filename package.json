{
  "name": "sequins",
  "version": "0.8.1",
  "description": "Mutable sequences and native data structures (Map, Set, List) following the Immutable.js API",
  "author": "conrad@burningpotato.com",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "main": "dist/_sequins.js",
  "module": "dist/sequins.mjs",
  "typings": "dist/sequins.d.ts",
  "scripts": {
    "prepublishOnly": "yarn build && yarn test",
    "build": "broccoli build dist --overwrite && npm run copy:dist",
    "build:pages": "gulp --gulpfile ./resources/gulpfile.js default",
    "copy:dist": "node ./resources/copy-dist-typedefs.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "broccoli": "^2.0.0-beta.4",
    "broccoli-babel-transpiler": "^7.0.0",
    "broccoli-cli": "^1.0.0",
    "broccoli-merge-trees": "^3.0.1",
    "broccoli-rollup": "^2.1.1",
    "browser-sync": "2.24.4",
    "browserify": "16.2.2",
    "del": "3.0.0",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-filter": "5.1.0",
    "gulp-header": "2.0.5",
    "gulp-less": "3.5.0",
    "gulp-size": "3.0.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-uglify": "2.1.0",
    "gulp-util": "3.0.8",
    "immutable": "^4.0.0-rc.10",
    "jest": "^23.6.0",
    "marked": "0.3.19",
    "react": "^0.12.0",
    "react-router": "^0.11.2",
    "react-tools": "0.13.3",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "run-sequence": "2.2.1",
    "typescript": "3.0.3",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "invariant": "^2.2.4",
    "iter-tools": "^6.1.1",
    "stable": "^0.1.8"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest_setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conartist6/sequins.git"
  },
  "keywords": [
    "mutable",
    "immutable",
    "map",
    "set",
    "list",
    "stable",
    "sort",
    "sequence",
    "seq",
    "iter-tools",
    "es6",
    "es2015"
  ],
  "bugs": {
    "url": "https://github.com/conartist6/sequins/issues"
  },
  "homepage": "https://github.com/conartist6/sequins#readme"
}
